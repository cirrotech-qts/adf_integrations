				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "\nSELECT \n    [No_]\nINTO \n    #RowsToKeep\nFROM \n    (SELECT \n        [No_],\n        ROW_NUMBER() OVER (ORDER BY [No_]) AS RowNum\n     FROM \n        [customer_from_BC]) AS BC_CTE\nWHERE \n    RowNum <= 10;\n\n\nDELETE FROM [customer_from_BC]\nWHERE [No_] NOT IN (SELECT [No_] FROM #RowsToKeep);\n\n\nDROP TABLE #RowsToKeep;\n",
